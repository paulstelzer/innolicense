{
  "name": "innolicense-api",
  "version": "1.0.3",
  "description": "License API",
  "author": "Paul Stelzer",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "prestart:prod": "rm -rf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "build": "tsc",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.2.2",
    "@nestjs/core": "^5.2.2",
    "@nestjs/passport": "^1.1.0",
    "@nestjs/swagger": "^2.4.5",
    "@nestjs/typeorm": "^5.2.0",
    "@nestjs/websockets": "^5.2.2",
    "axios": "^0.18.0",
    "bcrypt": "^2.0.1",
    "body-parser": "^1.18.3",
    "cheerio": "^1.0.0-rc.2",
    "class-validator": "^0.9.1",
    "dotenv": "^6.0.0",
    "entropy-string": "^4.1.0",
    "express-session": "^1.15.6",
    "fastify-formbody": "^2.0.1",
    "jsonwebtoken": "^8.3.0",
    "mysql": "^2.16.0",
    "node-fetch": "^2.2.0",
    "nodemailer": "^4.6.8",
    "passport": "^0.4.0",
    "passport-http-bearer": "^1.0.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.12",
    "request": "^2.88.0",
    "request-promise": "^4.2.2",
    "request-promise-cache": "^2.0.0",
    "rxjs": "^6.2.2",
    "typeorm": "^0.2.7"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.2.2",
    "@types/bcrypt": "^2.0.0",
    "@types/cheerio": "^0.22.9",
    "@types/express": "^4.0.39",
    "@types/jest": "^23.3.1",
    "@types/node": "^10.9.2",
    "@types/node-fetch": "^2.1.2",
    "@types/nodemailer": "^4.6.2",
    "@types/passport": "^0.4.6",
    "@types/passport-jwt": "^3.0.1",
    "@types/request-promise": "^4.1.42",
    "@types/supertest": "^2.0.5",
    "@types/uuid": "^3.4.3",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "supertest": "^3.0.0",
    "ts-jest": "^23.1.4",
    "ts-loader": "^4.5.0",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1",
    "webpack": "^4.17.1",
    "webpack-cli": "^3.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": ["js", "json", "ts"],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage"
  }
}